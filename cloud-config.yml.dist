#cloud-config
coreos: 
  etcd: 
    discovery: "https://discovery.etcd.io/TOKEN"
    addr: "$public_ipv4:4001"
    peer-addr: "$private_ipv4:7001"
  units: 
    - command: start
      name: etcd.service
    - command: start
      name: fleet.service
    - command: start
      name: docker.service
users: 
  - name: core
    coreos-ssh-import-github: smokeymonkey
